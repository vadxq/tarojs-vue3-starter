{
  "name": "tarojs-vue3-starter",
  "version": "0.1.0",
  "private": true,
  "description": "tarojs-vue3-starter",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "NODE_ENV=production taro build --type weapp",
    "build:swan": "NODE_ENV=production taro build --type swan",
    "build:alipay": "NODE_ENV=production taro build --type alipay",
    "build:tt": "NODE_ENV=production taro build --type tt",
    "build:h5": "NODE_ENV=production taro build --type h5",
    "build:rn": "NODE_ENV=production taro build --type rn",
    "build:qq": "NODE_ENV=production taro build --type qq",
    "build:quickapp": "NODE_ENV=production taro build --type quickapp",
    "build:kwai": "NODE_ENV=production taro build --type kwai",
    "buildtest:weapp": "NODE_ENV=development taro build --type weapp",
    "buildtest:swan": "NODE_ENV=development taro build --type swan",
    "buildtest:alipay": "NODE_ENV=development taro build --type alipay",
    "buildtest:tt": "NODE_ENV=development taro build --type tt",
    "buildtest:h5": "NODE_ENV=development taro build --type h5",
    "buildtest:rn": "NODE_ENV=development taro build --type rn",
    "buildtest:qq": "NODE_ENV=development taro build --type qq",
    "buildtest:quickapp": "NODE_ENV=development taro build --type quickapp",
    "buildtest:kwai": "NODE_ENV=development taro build --type kwai",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "dev:kwai": "npm run build:kwai -- --watch"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "vadxq",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/nutui-taro": "^3.0.3",
    "@tarojs/components": "3.3.7",
    "@tarojs/plugin-html": "3.3.7",
    "@tarojs/runtime": "3.3.7",
    "@tarojs/taro": "3.3.7",
    "lottie-miniapp": "^1.9.1",
    "lottie-miniprogram": "^1.0.12",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@tarojs/cli": "3.3.7",
    "@tarojs/mini-runner": "3.3.7",
    "@tarojs/plugin-platform-kwai": "^1.0.0",
    "@tarojs/webpack-runner": "3.3.7",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.x",
    "@typescript-eslint/parser": "^2.x",
    "@vue/compiler-sfc": "^3.0.0",
    "axios": "^0.21.4",
    "babel-preset-taro": "3.3.7",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^6.8.0",
    "eslint-config-taro": "3.3.7",
    "eslint-plugin-vue": "^7.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "md5-file": "^5.0.0",
    "minimist": "^1.2.5",
    "prettier": "^2.4.0",
    "qr-image": "^3.2.0",
    "shelljs": "^0.8.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^3.7.0",
    "vue-loader": "^16.0.0-beta.8"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "prettier . -w",
    "*.{ts,tsx,js,vue}": "eslint . --fix",
    "*.{vue,css,scss,less,sass}": "stylelint --fix",
    "*": "prettier . -w -u"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  }
}
