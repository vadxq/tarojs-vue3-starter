{
  "name": "tarojs-vue3-starter",
  "version": "1.0.0",
  "private": true,
  "description": "tarojs多平台小程序",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "None",
    "framework": "Vue3"
  },
  "author": "vadxq",
  "license": "MIT",
  "homepage": "https://github.com/vadxq/tarojs-vue3-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/vadxq/tarojs-vue3-starter.git"
  },
  "type": "module",
  "scripts": {
    "postinstall": "weapp-tw patch",
    "build:weapp": "npx taro build --type weapp",
    "build:swan": "npx taro build --type swan",
    "build:alipay": "npx taro build --type alipay",
    "build:tt": "npx taro build --type tt",
    "build:h5": "npx taro build --type h5",
    "build:rn": "npx taro build --type rn",
    "build:qq": "npx taro build --type qq",
    "build:jd": "npx taro build --type jd",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "prepare": "husky",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.mjs",
    "lint:pretty": "npx pretty-quick --staged",
    "lint": "pnpm lint:lint-staged && pnpm lint:pretty"
  },
  "browserslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/runtime": "^7.26.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@nutui/nutui-taro": "^4.3.13",
    "@nutui/touch-emulator": "^1.0.0",
    "@tarojs/components": "4.0.9",
    "@tarojs/helper": "4.0.9",
    "@tarojs/plugin-framework-vue3": "4.0.9",
    "@tarojs/plugin-html": "^4.0.9",
    "@tarojs/plugin-platform-alipay": "4.0.9",
    "@tarojs/plugin-platform-h5": "4.0.9",
    "@tarojs/plugin-platform-jd": "4.0.9",
    "@tarojs/plugin-platform-qq": "4.0.9",
    "@tarojs/plugin-platform-swan": "4.0.9",
    "@tarojs/plugin-platform-tt": "4.0.9",
    "@tarojs/plugin-platform-weapp": "4.0.9",
    "@tarojs/runtime": "4.0.9",
    "@tarojs/service": "4.0.9",
    "@tarojs/shared": "4.0.9",
    "@tarojs/taro": "4.0.9",
    "mockjs": "^1.1.0",
    "pinia": "^2.2.6",
    "sass": "^1.80.6",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@nutui/auto-import-resolver": "^1.0.0",
    "@tarojs/cli": "4.0.9",
    "@tarojs/plugin-mini-ci": "^4.0.9",
    "@tarojs/vite-runner": "4.0.9",
    "@types/node": "^22.9.0",
    "@types/sass": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "autoprefixer": "^10.4.20",
    "babel-preset-taro": "4.0.9",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^10",
    "eslint-config-taro": "4.0.9",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-taro": "^3.3.20",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.4.47",
    "postcss-rem-to-responsive-pixel": "^6.0.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "pretty-quick": "^4.1.1",
    "stylelint": "^16.10.0",
    "tailwindcss": "^3.4.14",
    "terser": "^5.36.0",
    "typescript": "^5.8.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^4.5.5",
    "vue-eslint-parser": "^9.4.3",
    "weapp-tailwindcss": "^3.6.2"
  }
}
